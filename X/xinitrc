export GTK_IM_MODULE=fcitx
export XMODIFIERS=@im=fcitx
export QT_IM_MODULE=fcitx

fcitx 

pulseaudio --start &

random_wallpapers

xrdb ~/.Xresources


session() {
  exec dbus-launch --exit-with-session "${@}"
}

cd
case "$2" in
  gnome)
    exec gnome-session ;;
  steam)
    openbox &
    session steam ;;
  xbmc)
    session "$2" ;;
  xmonad)
    session "$2" ;;
  *)
    session "${2:-xmonad}" ;;
esac

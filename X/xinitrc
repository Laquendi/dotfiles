xmodmap ~/.Xmodmap
xmodmap ~/layouts/dvorak
nappis.zsh
xrdb ~/.Xresources

pulseaudio --start &

export GTK_IM_MODULE=ibus
export XMODIFIERS=@im=ibus
export QT_IM_MODULE=ibus

ibus-daemon -drx

xrandr --setprovideroutputsource modesetting NVIDIA-0
xrandr --auto
#xrandr --output eDP-1-0 --off

session() {
  exec dbus-launch --exit-with-session "${@}"
}

cd
case "$2" in
  gnome)
    exec gnome-session ;;
  steam)
    openbox &
    nitrogen --restore
    session steam ;;
  xbmc)
    session "$2" ;;
  xmonad)
    session "$2" ;;
  *)
    nitrogen --restore
    session "${2:-xmonad}" ;;
esac

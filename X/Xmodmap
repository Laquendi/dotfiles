clear lock
!clear control
!clear mod1
!clear mod2
!clear mod3
!clear mod4 
!clear mod5

keycode 66  = Escape
!keycode 64  = Return
!keycode 108 = BackSpace
!keycode 135 = Control_R
!keycode 105 = Mode_switch
!keycode 36  = Caps_Lock
!keycode 22  = Alt_L

keycode  94 = adiaeresis        odiaeresis          aring                   Aring

!add mod1 = Alt_L
!add mod2 = Mode_switch
!add mod4 = Super_L Multi_key
!add control = Control_L Control_R

*font:      xft:inconsolata:pixelsize=15,xft:IPAPGothic

Rxvt.xftAntialias:     true
Rxvt.scrollBar:        False
Rxvt.saveLines: 0

Rxvt*transparent: false
Rxvt*externalBorder: 0
Rxvt*internalBorder: 0
Rxvt*borderless: true

URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,matcher
URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
URvxt.url-select.button:    3
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

*background: #ffffff
*foreground: #4d4d4c

